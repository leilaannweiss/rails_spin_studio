<h4>Classes With Guests</h4>
  <ul class="list-group">
    <% @reservations.with_guests.each do |reservation| %>
      <li class="list-group-item"><%= reservation.user.name %> Guest count: <%= reservation.guests %></li>
    <% end %>
  </ul>

  <h4>Classes Without Guests</h4>
  <ul class="list-group">
    <% @reservations.without_guests.each do |reservation| %>
      <li class="list-group-item"><%= reservation.user.name %></li>
      <% end %>
    </ul>
</div>
<%= link_to "Back", user_path(current_user), role: "button", class: "btn btn-primary btn-sm" %>
